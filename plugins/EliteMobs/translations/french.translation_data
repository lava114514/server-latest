  trackHoverLines: '&aCliquez pour suivre la quête !'
  untrackTextLines: '&4&l[Ne plus traquer]'
  untrackHoverLines: '&cCliquez pour annuler le suivi de quête !'
  completedTextLines: '&2&l[Soumettre!]'
  completedHoverLines: '&aCliquez pour suivre la quête !'
  turnedInTextLines: '&8[Terminé !]'
  turnedInHoverLines: '&8Déjà suivie!'
