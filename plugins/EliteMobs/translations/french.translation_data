  killQuestDefaultSummaryLine: '&c➤Tuer $name:$color$current&0/$color$target'
  fetchQuestDefaultSummaryLine: '&c➤Obtenir $name:$color&$current&0/$color$target'
  dialogQuestDefaultSummaryLine: '&c➤Allez parler à $name $location'
